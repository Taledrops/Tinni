<?xml version="1.0" encoding="utf-8"?>

<!--
 * <h1>Dark To Transparent</h1>
 * Gradient from dark to transparent
 *
 * @author Nassim Amar
 * @version 1.0
 * @since 27.06.2020
-->

<shape xmlns:android="http://schemas.android.com/apk/res/android">
    <gradient
        android:startColor="@color/colorPrimaryDarkTransparentLight"
        android:centerColor="@color/colorPrimaryDarkTransparentLighter"
        android:endColor="@android:color/transparent"
        android:angle="90"
        android:dither="true"
        />
</shape>